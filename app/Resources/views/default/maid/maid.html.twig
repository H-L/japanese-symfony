{% extends 'default/base.html.twig' %}
{% block body %}
    <h1>Maid</h1>
    <ul>
        {% for maid in maids %}
            <li>
                <img src="{{ maid.profilePicture }}" width="150px "alt="">
                <p>name : {{ maid.name }}</p>
                <a href="{{ path('_maid_show', {'id': maid.id }) }}">Full info on {{ maid.name }}</a>
            </li>
        {% endfor %}
    </ul>
{% endblock %}